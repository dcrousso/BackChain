{
	"manifest_version": 2,
	"name": "__MSG_appName__",
	"version": "1.0.2",
	"default_locale": "en",
	"description": "__MSG_appDescription__",
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"background": {
		"scripts": [ "scripts/background.js" ]
	},
	"content_scripts": [ {
		"matches": ["<all_urls>"],
		"js": ["scripts/backchain.js"],
		"run_at": "document_end"
	} ],
	"permissions": [
		"tabs"
	]
}